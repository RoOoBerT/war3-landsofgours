package DamageType2

// Standard imports
import LinkedList

// Custom imports

/** Represents a type of damage */
public class DamageType2
    private constant int index
    private constant string name

    private static constant LinkedList<DamageType2> LIST = new LinkedList<DamageType2>() 

    construct (string name)
        this.index = LIST.size()
        this.name = name

        // Register this damage type
        LIST.addAt(this, this.index)

